#include <vector>
using namespace std;

int main() {
	int i,j,t,x,n;
	cin>>t;
	while(t--)
	{
	    cin>>n;
	    vector<int>v;
	    for(i=0;i<n;i++)
	    {
	        cin>>x;
	        v.push_back(x);
	    }
	    i=0,j=v.size()-1;
	    int cnt=0;
	    while(i<j)
	    {
	        if(v[i]==v[j])i++,j--;
	        else if(v[i]>v[j])
	        {
	            v[j-1]+=v[j];
	            cnt++;
	            j--;
	        }
	        else {
	            v[i+1]+=v[i];
	            cnt++;
	            i++;
	        }
	    }
	    cout<<cnt<<endl;
	}
	return 0;
}
